{
    "manifest_version": 3,
    "name": "WhatsApp Web Wallpaper",
    "version": "1.1",
    "browser_specific_settings": {
        "gecko": { "id": "your-id@example.com" }
    },
    "web_accessible_resources": [{
        "resources": ["wallpaper.jpg"],
        "matches": ["https://web.whatsapp.com/*"]
    }],
    "content_scripts": [{
        "matches": ["https://web.whatsapp.com/*"],
        "js": ["wallpaperSetter.js"]
    }]
}